{
  "builds": [
    {
      "src": "interface/server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "interface/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "interface/client/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "interface/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "interface/client/build/$1"
    }
  ]
}
